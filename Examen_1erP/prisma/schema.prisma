// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Funcion{
	id				Int @id @default(autoincrement())
  name			String
  returnType		String
  detail			String
  status			String
  parameters		Parametros[]
}

model Parametros{
	id				Int @id @default(autoincrement())
	descripcion		String
	type			String
	defaultValue	String
	functionId		Int
	funcion			Funcion	@relation(fields: [functionId], references: [id])
}